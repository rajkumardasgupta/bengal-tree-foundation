{
  "hosting": {
    "public": "public",
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|png|gif|svg|webp)",
        "headers": [{ "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
  "redirects": [ {
    "source": "https://www.treefoundationbengal.web.app",
    "destination": "https://treefoundationbengal.web.app",
    "type": 301
  } ]
  }
}

